[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
[push]
	default = current
[diff]
	tool = meld
[merge]
	tool = meld
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.cvsignore
